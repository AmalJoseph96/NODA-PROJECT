                                            <td><%= order.payementMethod %></td>
                                            <td><%= order.totalAmount %></td>
                                            <td><%= new Date(order.orderDate).toLocaleDateString() %></td>
                                            <td><span class="badge rounded-pill <%= order.payementStatus === 'Paid' ? 'alert-success' : 'alert-warning' %>"><%= order.payementStatus %></span></td>
